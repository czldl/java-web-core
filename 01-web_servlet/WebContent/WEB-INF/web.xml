<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>01-web_servlet</display-name>

	<!-- Servlet生命周期 -->
	<servlet>
		<servlet-name>liftServlet</servlet-name>
		<servlet-class>net.codingme.servlet.LiftServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>liftServlet</servlet-name>
		<url-pattern>/liftServlet</url-pattern>
	</servlet-mapping>
	
	<!-- 第一个Servlet -->
	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>net.codingme.servlet.FirstServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/firstServlet</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>